{
  "manifest_version": 2,
  "name": "View Images",
  "version": "1.0",
  "description": "Restore the View Image button on Google Images",
  "author": "weiwei",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "include_globs": ["http://www.google.*/*", "https://www.google.*/*"],
      "js": ["jquery-3.3.1.min.js", "script.js"]
    }
  ]
}
